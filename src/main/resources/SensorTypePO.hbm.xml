<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.yanxin.iot.mariadb.beans.SensorTypePO" table="sensor_type" schema="iotdb">
        <id name="id" column="id"/>
        <property name="type" column="type"/>
        <property name="createdTime" column="created_time"/>
        <set name="sensorsById" inverse="true">
            <key>
                <column name="type_id"/>
            </key>
            <one-to-many not-found="ignore" class="com.yanxin.iot.mariadb.beans.SensorPO"/>
        </set>
    </class>
</hibernate-mapping>